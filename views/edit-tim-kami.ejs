<html lang="en">
  <head>
    <%- include('head'); -%>
    <!-- Include Quill stylesheet -->
    <link href="/css/quill.snow.css" rel="stylesheet">
    <link href="//cdn.quilljs.com/1.3.6/quill.core.css" rel="stylesheet">
  </head>
  <body id="page-top">
    <div class="container">
      
      <h3><a href="/admin-home">Home</a> / <a href="/admin-tim-kami">Admin Tim Kami</a> / Edit Tim Kami</h3>
      <div id="edit-tim-kami" class="container">
        <form id="edit-kami-form" action="/edit-tim-request/<%- tim.uuid -%>" method="post" enctype="multipart/form-data">
          <div class="inputs-group">
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Foto
              </div>
              <div class="col-lg-10 col-sm-12">
                <img src="/public/uploads/<%= tim.foto %>" alt="" height="200px">
                <input type="file" placeholder="Image" name="tImage" accept="image/png, image/jpeg">
              </div>
            </div>
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Nama
              </div>
              <div class="col-lg-10 col-sm-12">
                : <input type="text" placeholder="Nama" name="tNama" value="<%= tim.nama %>" >
              </div>
            </div>
            <div class="row">
              <div class="col-12">
                <nav>
                  <div class="nav nav-tabs" id="nav-tab" role="tablist">
                    <a class="nav-item nav-link active" id="nav-bahasa-tab" data-toggle="tab" href="#nav-bahasa" role="tab" aria-controls="nav-bahasa" aria-selected="true">Bahasa</a>
                    <a class="nav-item nav-link" id="nav-english-tab" data-toggle="tab" href="#nav-english" role="tab" aria-controls="nav-english" aria-selected="false">English</a>
                  </div>
                </nav>
                <br>
                <div class="tab-content" id="nav-tabContent">
                  <div class="tab-pane fade show active" id="nav-bahasa" role="tabpanel" aria-labelledby="nav-bahasa-tab">
                    <div class="row">
                      <div class="col-lg-2 col-sm-12">
                        Jabatan
                      </div>
                      <div class="col-lg-10 col-sm-12">
                        : <input type="text" placeholder="Jabatan" name="tJabatan" value="<%= tim.jabatan %>" required>
                      </div>
                    </div>
                    <br>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Keterangan
                      </div>
                      <div class="col-lg-10 col-sm-12" id="tk-keterangan">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="keterangan-editor">
                          <%- tim.keterangan -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Overview
                      </div>
                      <div class="col-lg-10 col-sm-12" id="tk-overview">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="overview-editor">
                          <%- tim.overview -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Riset
                      </div>
                      <div class="col-lg-10 col-sm-12" id="tk-riset">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="riset-editor">
                          <%- tim.riset -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Publikasi
                      </div>
                      <div class="col-lg-10 col-sm-12" id="tk-publikasi">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="publikasi-editor">
                          <%- tim.publikasi -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Supervisi
                      </div>
                      <div class="col-lg-10 col-sm-12" id="tk-supervisi">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="supervisi-editor">
                          <%- tim.supervisi -%>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="tab-pane fade" id="nav-english" role="tabpanel" aria-labelledby="nav-english-tab">
                    
                    <div class="row">
                      <div class="col-lg-2 col-sm-12">
                        Jabatan
                      </div>
                      <div class="col-lg-10 col-sm-12">
                        : <input type="text" placeholder="Jabatan" name="entJabatan" value="<%= tim.enjabatan %>" required>
                      </div>
                    </div>
                    <br>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Keterangan
                      </div>
                      <div class="col-lg-10 col-sm-12" id="entk-keterangan">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="enketerangan-editor">
                          <%- tim.enketerangan -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Overview
                      </div>
                      <div class="col-lg-10 col-sm-12" id="entk-overview">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="enoverview-editor">
                          <%- tim.enoverview -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Riset
                      </div>
                      <div class="col-lg-10 col-sm-12" id="entk-riset">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="enriset-editor">
                          <%- tim.enriset -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Publikasi
                      </div>
                      <div class="col-lg-10 col-sm-12" id="entk-publikasi">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="enpublikasi-editor">
                          <%- tim.enpublikasi -%>
                        </div>
                      </div>
                    </div>
                    <div class="row tk-textarea">
                      <div class="col-lg-2 col-sm-12">
                        Supervisi
                      </div>
                      <div class="col-lg-10 col-sm-12" id="entk-supervisi">
                        <!-- Create the toolbar container -->
                        <div id="toolbar">
                        </div>
                        <!-- Create the editor container -->
                        <div id="ensupervisi-editor">
                          <%- tim.ensupervisi -%>
                        </div>
                      </div>
                    </div>
                    
                  </div>
                </div>
              </div>
            </div>
            
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Facebook
              </div>
              <div class="col-lg-10 col-sm-12">
                : <input type="text" placeholder="Facebook" name="tFacebook" value="<%- tim.facebook -%>">
              </div>
            </div>
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Instagram
              </div>
              <div class="col-lg-10 col-sm-12">
                : <input type="text" placeholder="Instagram" name="tInstagram" value="<%- tim.instagram -%>">
              </div>
            </div>
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Twitter
              </div>
              <div class="col-lg-10 col-sm-12">
                : <input type="text" placeholder="Twitter" name="tTwitter" value="<%- tim.twitter -%>">
              </div>
            </div>
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Youtube
              </div>
              <div class="col-lg-10 col-sm-12">
                : <input type="text" placeholder="Youtube" name="tYoutube" value="<%- tim.youtube -%>">
              </div>
            </div>
            <div class="row">
              <div class="col-lg-2 col-sm-12">
                Urutan
              </div>
              <div class="col-lg-10 col-sm-12">
                : <input type="number" placeholder="Urutan" name="tUrutan" value="<%- tim.urutan -%>" required>
              </div>
            </div>
            
          </div>
          <%= typeof ermes != 'undefined' ? ermes : '' %>
          <input type="submit" class="btn btn-prime" value="Submit" id="tk-input-submit">
          <input type="hidden" id="input-keterangan" value="" name="tKeterangan">
          <input type="hidden" id="input-overview" value="" name="tOverview">
          <input type="hidden" id="input-riset" value="" name="tRiset">
          <input type="hidden" id="input-publikasi" value="" name="tPublikasi">
          <input type="hidden" id="input-supervisi" value="" name="tSupervisi">
          <input type="hidden" id="oldImg" value="<%= tim.foto %>" name="tOldImg">
        </form>
      </div>
      
      <!--/ Section Contact-Footer Star /-->
      <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>
      <div id="preloader"></div>
      <!-- JavaScript Libraries -->
      <script src="/lib/jquery/jquery.min.js"></script>
      <script src="/lib/jquery/jquery-migrate.min.js"></script>
      <script src="/lib/popper/popper.min.js"></script>
      <script src="/lib/bootstrap/js/bootstrap.min.js"></script>
      <script src="/lib/easing/easing.min.js"></script>
      <script src="/lib/counterup/jquery.waypoints.min.js"></script>
      <script src="/lib/counterup/jquery.counterup.js"></script>
      <script src="/lib/owlcarousel/owl.carousel.min.js"></script>
      <script src="/lib/lightbox/js/lightbox.min.js"></script>
      <script src="/lib/typed/typed.min.js"></script>
      
      <!-- Contact Form JavaScript File -->
      <script src="/contactform/contactform.js"></script>
      <!-- Template Main Javascript File -->
      <script src="/js/main.js"></script>
      <!-- Initialize Quill editor -->
      <!-- Include the Quill library -->
      <script src="https://cdn.quilljs.com/1.0.0/quill.js"></script>
      <script src="/js/add-tim.js"></script>
      <!--/ Section Contact-footer End /-->
      
    </div>
  </body>
</html>
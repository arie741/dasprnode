<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('head'); -%>
  </head>
  <body id="page-top" class="admin-home-body">
    <div class="container">
      <div class="row" id="wc-admin">
        <div class="col-sm-12 col-lg-12">
          <div class="card align-middle">
            <div class="card-body">
              
              <h3 class="text-center"><img src="/img/logo/DASPR.png" height="100%" width="56px" alt="">Welcome Admin!</h3>
              
            </div>
          </div>
        </div>
      </div>
      <br>
      <div class="row" id="adm-home">
        <div class="col-lg-3" id="mng-pub">
          <a href="/admin-publication">
            <video muted loop id="mng-pub-video">
              <source src="/videos/daspr1.mp4" type="video/mp4">
            </video>
            <h3 class="text-center">Manage Publication</h3>
          </a>
        </div>
        <div class="col-lg-3" id="mng-home-slider">
          <a href="/add-slider">
            <video muted loop id="mng-home-slider-video">
              <source src="/videos/daspr2.mp4" type="video/mp4">
            </video>
            <h3 class="text-center">Manage Home Slider</h3>
          </a>
        </div>
        <div class="col-lg-3" id="mng-news-events">
          <a href="/admin-news-and-events">
            <video muted loop id="mng-news-events-video">
              <source src="/videos/daspr3.mp4" type="video/mp4">
            </video>
            <h3 class="text-center">Manage News and Events</h3>
          </a>
        </div>
        <div class="col-lg-3" id="mng-tim-kami">
          <a href="/admin-tim-kami">
            <video muted loop id="mng-tim-kami-video">
              <source src="/videos/daspr4.mp4" type="video/mp4">
            </video>
            <h3 class="text-center">Manage Tim Kami</h3>
          </a>
        </div>
      </div>
      <br>
      <br>
      <br>
      <div class="row">
        <div class="col-lg-12">
          <a class="btn btn-prime" href="/logout">Logout</a>
        </div>
      </div>
    </div>
    
    <!--/ Section Contact-Footer Star /-->
    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>
    <div id="preloader"></div>
    <!-- JavaScript Libraries -->
    <script src="lib/jquery/jquery.min.js"></script>
    <script src="lib/jquery/jquery-migrate.min.js"></script>
    <script src="lib/popper/popper.min.js"></script>
    <script src="lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/counterup/jquery.waypoints.min.js"></script>
    <script src="lib/counterup/jquery.counterup.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/lightbox/js/lightbox.min.js"></script>
    <script src="lib/typed/typed.min.js"></script>
    
    <!-- Contact Form JavaScript File -->
    <script src="contactform/contactform.js"></script>
    <!-- Template Main Javascript File -->
    <script src="js/main.js"></script>
    <!--/ Section Contact-footer End /-->
    <script>
    $("#mng-pub video").hover(
    function(){
    var video = document.getElementById("mng-pub-video");
    video.play();},
    function(){
    var video = document.getElementById("mng-pub-video");
    video.pause();}
    );
    $("#mng-home-slider video").hover(
    function(){
    var video = document.getElementById("mng-home-slider-video");
    video.play();},
    function(){
    var video = document.getElementById("mng-home-slider-video");
    video.pause();}
    );
    $("#mng-news-events video").hover(
    function(){
    var video = document.getElementById("mng-news-events-video");
    video.play();},
    function(){
    var video = document.getElementById("mng-news-events-video");
    video.pause();}
    );
    $("#mng-tim-kami video").hover(
    function(){
    var video = document.getElementById("mng-tim-kami-video");
    video.play();},
    function(){
    var video = document.getElementById("mng-tim-kami-video");
    video.pause();}
    );    
    </script>
  </body>
</html>
<html lang="en">
  <head>
    <%- include('head'); -%>
    <link href="/css/quill.snow.css" rel="stylesheet">
  </head>
  <body id="page-top">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <div class="card align-middle">
            <div class="card-body">
              <h3><a href="/admin-home">Home</a> / Admin Tim Kami</h3>
              <form action="/admin-tk-search-request" method="post">
                <input type="text" placeholder="Search" name="pSearch">
                <input class="btn btn-prime" type="submit" value="Search">
                <a class="btn btn-prime"  href="/admin-tim-kami">Clear</a>
              </form>
              <a class="btn btn-prime" href="/admin-add-tim">Tambahkan Tim</a>
              <br>
              <br>
              
              <section id="timkami" class="admin-timkami">
                <div class="row">
                  <div class="col-12">
                    <nav>
                      <div class="nav nav-tabs" id="nav-tab" role="tablist">
                        <a class="nav-item nav-link active" id="nav-bahasa-tab" data-toggle="tab" href="#nav-bahasa" role="tab" aria-controls="nav-bahasa" aria-selected="true">Bahasa</a>
                        <a class="nav-item nav-link" id="nav-english-tab" data-toggle="tab" href="#nav-english" role="tab" aria-controls="nav-english" aria-selected="false">English</a>
                      </div>
                    </nav>
                    <div class="tab-content" id="nav-tabContent">
                      <div class="tab-pane fade show active" id="nav-bahasa" role="tabpanel" aria-labelledby="nav-bahasa-tab">
                        <!--TAB BAHASA-->
                        <div class="row">
                          <% timkami.forEach(function(tim){%>
                          <div class="col-lg-4">
                            <div class="card" style="width: 18rem;">
                              <a href="#" data-toggle="modal" data-target="#timModal<%= tim.urutan %>">
                                <img src="/public/uploads/<%= tim.foto %>" class="card-img-top" alt="">
                              </a>
                              <div class="card-body">
                                <h5 class="card-title text-center"><a href="#"  data-toggle="modal" data-target="#timModal<%= tim.urutan %>"><%= tim.nama %></a></h5>
                                <p class="card-text text-center"><a href="#"  data-toggle="modal" data-target="#timModal<%= tim.urutan %>"><%= tim.jabatan %></a></p>
                                <div class="socials">
                                  <ul>
                                    <li><a href="<%= tim.facebook %>"><span class="ico-circle center"><i class="ion-social-facebook"></i></span></a></li>
                                    <li><a href="<%= tim.instagram %>"><span class="ico-circle center"><i class="ion-social-instagram"></i></span></a></li>
                                    <li><a href="<%= tim.twitter %>"><span class="ico-circle center"><i class="ion-social-twitter"></i></span></a></li>
                                    <li><a href="<%= tim.youtube %>"><span class="ico-circle center"><i class="ion-social-youtube"></i></span></a></li>
                                  </ul>
                                </div>
                                <a href="#" data-toggle="modal" data-target="#timModal<%= tim.urutan %>" class="btn btn-prime center">Detail</a>
                              </div>
                            </div>
                            <br>
                            <div class="edit-btn">
                              <a href="/edit-tim/<%= tim.uuid %>" class="btn btn-prime">Edit</a>
                              <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#delcon<%= tim.uuid %>">Delete</button>
                            </div>
                            <br>
                          </div>
                          
                          <%});%>
                        </div>
                      </div>
                      <div class="tab-pane fade" id="nav-english" role="tabpanel" aria-labelledby="nav-english-tab">
